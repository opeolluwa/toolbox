{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "cli": {
    "description": "utills",
    "longDescription": "a quicker way to do things",
    "name": "utills",
    "version": "0.5.14",
    "args": [],
    "subcommands": {
      "store": {
        "description": "store data as a key-value pair",
        "longDescription": "store data as a key-value pair",
        "args": [
          {
            "name": "key",
            "shore": "k",
            "description": "the key to store the data under",
            "longDescription": "the key to store the data under",
            "required": true
          },
          {
            "name": "value",
            "shore": "v",
            "description": "the value to store",
            "longDescription": "the value to store",
            "required": true
          }
        ],
        "subcommands": {
          "get": {
            "description": "get the value of a key",
            "longDescription": "get the value of a key",
            "args": [
              {
                "name": "key",
                "shore": "k",
                "description": "the key to get the value of",
                "longDescription": "the key to get the value of",
                "required": true
              }
            ]
          },
          "delete": {
            "description": "delete a key-value pair",
            "longDescription": "delete a key-value pair",
            "args": [
              {
                "name": "key",
                "shore": "k",
                "description": "the key to delete",
                "longDescription": "the key to delete",
                "required": true
              }
            ]
          }
        }
      },
      "ignore": {},
      "mailto": {},
      "readme": {}
    }
  },
  "package": {
    "productName": "utills",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.utills.dev",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "utills",
        "width": 800,
        "height": 600
      }
    ]
  }
}
